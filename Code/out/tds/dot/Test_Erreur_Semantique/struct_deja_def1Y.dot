digraph "tds"{

	node [fontname="Arial"];

	N1 [shape=record label="{ structure : list 1
	| { val | attribut | int | depl }
	}"];

	N2 [shape=record label="{ structure : list 1
	| { l | attribut | struct list | depl }
	}"];

	N3 [shape=record label="{ fonction : int main 1
	}"];

	N0 [shape=record label="{ global 0
	}"];


	N0 -> N1; 
	N0 -> N2; 
	N0 -> N3; 
}
