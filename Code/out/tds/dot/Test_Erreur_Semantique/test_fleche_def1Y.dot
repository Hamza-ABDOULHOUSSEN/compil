digraph "tds"{

	node [fontname="Arial"];

	N1 [shape=record label="{ structure : struct person 1
	| { age | attribut | int | 0 }
	}"];

	N2 [shape=record label="{ fonction : int main 1
	| { p | attribut | struct person | 0 }
	}"];

	N0 [shape=record label="{ global 0
	}"];


	N0 -> N1; 
	N0 -> N2; 
}
