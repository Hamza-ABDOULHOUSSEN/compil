digraph "tds"{

	node [fontname="Arial"];

	N3 [shape=record label="{ bloc : Bloc If  3
	}"];

	N2 [shape=record label="{ bloc : Bloc If  2
	}"];

	N1 [shape=record label="{ fonction : int main 1
	| { x | attribut | int | depl }
	}"];

