digraph "tds"{

	node [fontname="Arial"];

	N1 [shape=record label="{ structure : nombre 1
	| { val | attribut | int | depl }
	}"];

	N2 [shape=record label="{ int fonction : main 1
	| { a | attribut | struct nombre | depl }
	}"];

	N0 [shape=record label="{ global 0
	}"];


	N0 -> N1; 
	N0 -> N2; 
}
