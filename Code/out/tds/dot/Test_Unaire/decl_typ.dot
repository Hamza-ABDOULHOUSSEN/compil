digraph "tds"{

	node [fontname="Arial"];

	N1 [shape=record label="{ structure : vide
	}"];

	N2 [shape=record label="{ structure : maison
	| { a | attribut | int | depl }
	| { b | attribut | int | depl }
	| { c | attribut | int | depl }
	| { a | attribut | maison | depl }
	| { b | attribut | maison | depl }
	| { c | attribut | maison | depl }
	| { a | attribut | int | depl }
	}"];

	N0 [shape=record label="{ global 0
	}"];


	N0 -> N1; 
	N0 -> N2; 
}
