digraph "tds"{

	node [fontname="Arial"];

	N1 [shape=record label="{ structure : struct vide 1
	}"];

	N2 [shape=record label="{ structure : struct maison 1
	| { a | attribut | int | 0 }
	| { b | attribut | int | 4 }
	| { c | attribut | int | 8 }
	| { a | attribut | struct maison | 12 }
	| { b | attribut | struct maison | 20 }
